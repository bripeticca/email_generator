#include <string.h>
#include <stdio.h>
#include <stdlib.h>

string email_generator(int num) {
  int count = num;
  string email_begin;
  string email_end; // make sure email end includes the dot
  string *emails = malloc(sizeof(string) * num);
  while (num > 0) {

    for(int i = 10; i > 0; --i){

    }
    emails[num - 1] = email_begin + "@" + email_end;
    --num;
  }

  for(int x = 0; x < count; ++x) {
    printf("%u", emails[x]);
  }

  free(emails);
  }
